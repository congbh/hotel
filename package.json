{
  "name": "hospitality-api",
  "version": "1.0.0",
  "description": "Hotel api gateway",
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp --gulpfile gulpfile.js",
    "start:prod": "cross-env NODE_ENV=production gulp --gulpfile gulpfile.js",
    "lint:fix": "eslint ./ --fix",
    "test": "lab -v -L -C -c -t 85",
    "test-cov-html": "lab -r html -o coverage.html",
    "dev:migrate": "./migrate-db-dev.sh",
    "test:migrate": "./migrate-db-test.sh",
    "dev:start": "./run-dev.sh",
    "dev:db:start": "docker-compose -f docker-compose.yml up -d postgres-db",
    "dev:uninstall": "docker-compose -f docker-compose.yml down --rmi all",
    "dev:up": "docker-compose -f docker-compose.yml up",
    "dev:build": "docker-compose -f docker-compose.yml build",
    "dev:down": "docker-compose -f docker-compose.yml down",
    "dev:api:connect": "docker exec -it api-gateway /bin/sh",
    "dev:db:connect": "docker exec -it postgres-db /bin/sh",
    "dev:db:psql": "psql -h localhost -p 5432 -U postgres -d hotel_dev",
    "dev:clean": "docker rmi $(docker images -f \"dangling=true\" -q)"
  },
  "author": "Bach Hung Cong",
  "license": "ISC",
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "code": "^5.2.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "gulp-nodemon": "^2.2.1",
    "lab": "^15.4.0",
    "sinon": "^4.4.8",
    "wreck": "^14.0.2"
  },
  "dependencies": {
    "awilix": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "boom": "^7.2.0",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-file": "^6.0.1",
    "good-http": "^6.1.3",
    "good-squeeze": "^5.0.2",
    "hapi": "^17.2.2",
    "hapi-swagger": "^9.1.1",
    "hoek": "^5.0.3",
    "inert": "^5.1.0",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.14.4",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "node-uuid": "^1.4.8",
    "pg": "^7.4.1",
    "vision": "^5.3.1"
  }
}
